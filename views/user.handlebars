<link href="justified-nav.css" rel="stylesheet">
<div class="container">

  <!-- The justified navigation menu is meant for single line per list item.
       Multiple lines will require custom code not provided by Bootstrap. -->
  <div class="masthead">
    <h3 class="text-muted"></h3>
    <nav>
      <ul class="nav nav-justified">
        <li class="active"><a href="#">Check Beds</a></li>
        <li><a href="#">Find a Shower Truck</a></li>
        <li><a href="#">Calendar</a></li>
        <li><a href="#">Find Nearest Facility</a></li>
        <li><a href="#">About</a></li>
        <li><a href="#">Contact</a></li>
      </ul>
    </nav>
  </div>
  <br><br><br>
  <h1>Shelter Facilities</h1>
  <table class="table">
    <tr>
      <td>Facility</td>
      <td>Available Beds</td>
      <td>Unit Number</td>
	  <td>Street Number</td>
	  <td>Address</td>
	  <td>City</td>
	  <td>State</td>
	  <td>Zip Code</td>
    </tr>
    {{#results}}
      <tr>
        <td>{{name}}</td>
        <td>{{available}}</td>
        <td>{{bed_total}}</td>
		<td>{{unitNum}}</td>
	    <td>{{street_num}}</td>
	    <td>{{Street_name}}</td>
	    <td>{{city}}</td>
	    <td>{{state}}</td>
	    <td>{{zip_code}}</td>
      </tr>  
    {{/results}}
  </table>
</div>